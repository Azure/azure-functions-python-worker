{
  "scriptFile": "__init__.py",

  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "authLevel": "anonymous"
    },
    {
      "direction": "in",
      "type": "cosmosDB",
      "name": "docs",
      "databaseName": "test",
      "collectionName": "items",
      "id": "cosmosdb-input-test",
      "leaseCollectionName": "leases",
      "connectionStringSetting": "AzureWebJobsCosmosDBConnectionString",
      "createLeaseCollectionIfNotExists": true
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
