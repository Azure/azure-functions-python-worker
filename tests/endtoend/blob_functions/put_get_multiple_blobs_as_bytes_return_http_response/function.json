{
  "scriptFile": "main.py",
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "authLevel": "anonymous"
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "inputfile1",
      "dataType": "binary",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-tests/shmem-test-bytes-1.txt"
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "inputfile2",
      "dataType": "binary",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-tests/shmem-test-bytes-2.txt"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputfile1",
      "dataType": "binary",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-tests/shmem-test-bytes-out-1.txt"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputfile2",
      "dataType": "binary",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-tests/shmem-test-bytes-out-2.txt"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
