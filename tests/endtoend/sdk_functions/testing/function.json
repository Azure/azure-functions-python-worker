{
  "scriptFile": "main.py",
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "authLevel": "anonymous"
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "client",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-tests/test-str.txt",
      "cardinality": "One",
      "properties": {
        "supportsDeferredBinding": "true"
      }
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
