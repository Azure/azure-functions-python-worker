{
  "scriptFile": "__init__.py",

  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "authLevel": "anonymous"
    },
    {
      "type": "eventHub",
      "name": "out",
      "direction": "out",
      "eventHubName": "python-worker-ci-eventhub-batch",
      "connection": "AzureWebJobsEventHubConnectionString"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
