{
  "scriptFile": "main.py",
  "disabled": false,

  "bindings": [
    {
      "type": "queueTrigger",
      "direction": "in",
      "name": "msg",
      "queueName": "testqueue-message-return",
      "connection": "AzureWebJobsStorage",
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "$return",
      "connection": "AzureWebJobsStorage",
      "path": "python-worker-tests/test-queue-blob-message-return.txt"
    }
  ]
}
